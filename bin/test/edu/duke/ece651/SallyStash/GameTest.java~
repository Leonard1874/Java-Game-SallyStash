public class GameTest {
  @Test
  public void test_() {
    Player p = new Player('A', 20, 10);
    p.place('R', 0, 1, 1);
    p.place('G', 0, 2, 1);
    p.place('P', 0, 3, 1);
    p.place('B', 0, 4, 1);
    p.place('R', 0, 1, 4);
    p.printBoard();
  }
}
